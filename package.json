{
  "name": "@sabl/db-driver",
  "version": "0.1.0",
  "description": "Components for implementing @sabl/db-api drivers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf coverage dist publish",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:view": "jest --coverageReporters text lcov && opener ./coverage/lcov-report/index.html",
    "test:refresh": "jest --coverageReporters text lcov",
    "test:ci": "jest --coverageReporters text cobertura json",
    "cbt": "pnpm clean && pnpm build && pnpm test",
    "postinstall": "husky install",
    "pub": "pnpm clean && pnpm build && node -r esm ./build/publish.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/libsabl/db-driver-js"
  },
  "contributors": [
    "Joshua Honig"
  ],
  "keywords": [
    "sabl",
    "db"
  ],
  "license": "MIT",
  "dependencies": {
    "@sabl/async": "^0.4.0",
    "@sabl/context": "^1.1.1",
    "@sabl/db-api": "^0.2.0",
    "@sabl/storage-pool": "^0.2.0",
    "@sabl/txn": "^0.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@sabl/async-test": "^0.4.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.45",
    "@types/rmfr": "^2.0.1",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "chalk": "^4.1.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esm": "^3.2.25",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "opener": "^1.5.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rmfr": "^2.0.0",
    "sqlite3": "^5.0.10",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
